package BasicSyntax;

import java.util.Scanner;

public class P06CharstoString {
    public static void main(String[] args){
        Scanner scanner=new Scanner(System.in);
        String result="";
        for(int i=0;i<3;i++){
            char first=scanner.next().charAt(0);
            result=result+first;
        }
        System.out.printf("%s",result);
    }
}
